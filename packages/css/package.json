{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@mincho/css",
  "version": "0.0.1",
  "type": "module",
  "typings": "./src/index.ts",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "yarn g:clean",
    "lint": "yarn g:lint",
    "fix": "yarn g:fix",
    "check": "yarn g:check"
  },
  "prettier": "prettier-config-custom",
  "eslintConfig": {
    "extends": [
      "custom"
    ],
    "parserOptions": {
      "project": [
        "./tsconfig.json",
        "./tsconfig.node.json"
      ]
    }
  },
  "dependencies": {
    "@mincho/transform-to-vanilla": "workspace:^"
  },
  "devDependencies": {
    "@vanilla-extract/css": "^1.13.0",
    "eslint-config-custom": "workspace:^",
    "prettier-config-custom": "workspace:^",
    "tsconfig-custom": "workspace:^",
    "vite-config-custom": "workspace:^"
  },
  "peerDependencies": {
    "@vanilla-extract/css": "*"
  }
}
