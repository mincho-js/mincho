{
  "extends": "tsconfig-custom/tsconfig.base.json",
  "compilerOptions": {
    "allowJs": true,
    "rootDir": ".",
    "outDir": "./.cache/tsc-eslint",
    "tsBuildInfoFile": "./.cache/tsc-eslint/tsconfig.eslint.tsbuildinfo",
    "types": ["node"]
  },
  "include": [
    "eslint.config.js",
    "src/**/*"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}
